{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/refresh",
      "schedule": "0 * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_TYPE": "postgres"
  }
}
